i = 1                          //  声明全局变量 i，赋初值 1

# test {n} ->                  //  test 函数定义
	@i = 0                     //  声明局部变量 i，赋初值 0
	* i < n -> ++i, $i *= i .  //  循环语句，当 i < n 时循环，局部变量 i 自增，全局变量 i 乘局部变量 i 并赋值
.                              //  test 函数定义结束

test{10}                       //  显式元组 {10} 调用函数 test
write_line{"10! = " + i}       //  调用原生函数，输出提示信息后接全局变量 i